# GitHub username
ghuser=

# GitHub password, quoted in case of special chars
ghpass=''

# the docroot / webroot on the remote server
webroot=public_html

# [optional] the command to execute drush on the remote server
remotedrush=drush

# IP or hostname of remote server
remotehost=

# GitHub repo; user (or org) name and repo name, separated by slash
repo=EncoreMultimedia/Encore-Dev-Scripts

# SSH username to use to connect to remote server
sshuser=

# SSH password, exported so sshpass can see it
export SSHPASS=
